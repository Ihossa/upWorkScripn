{
  "name": "SupWorker",
  "description": "Support to Upwork",
  "version": "2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.upwork.com/ab/proposals/job/*"],
      "js": ["message.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/upWork.png",
      "32": "/images/upWork.png",
      "48": "/images/upWork.png",
      "128": "/images/upWork.png"
    }
  },
  "icons": {
    "16": "/images/upWork.png",
    "32": "/images/upWork.png",
    "48": "/images/upWork.png",
    "128": "/images/upWork.png"
  }
}